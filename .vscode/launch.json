{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug w/ Qt6 Printers",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build/bin/pgmodeler",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}/build",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
      {
            "description": "Enable pretty-printing for gdb",
            "text": "-enable-pretty-printing",
            "ignoreFailures": true
        },
        {
            "description": "Set Disassembly Flavor to Intel",
            "text": "-gdb-set disassembly-flavor intel",
            "ignoreFailures": true
        }
      ]
    }
  ]
}